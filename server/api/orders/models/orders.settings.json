{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Orders",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "total": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 1
    },
    "products": {
      "type": "json",
      "required": true
    },
    "address": {
      "type": "richtext"
    },
    "charge_id": {
      "type": "string",
      "required": true
    },
    "token": {
      "type": "string"
    },
    "user": {
      "via": "orders",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
