{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "rating": {
      "type": "decimal",
      "max": 5,
      "min": 1,
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 1,
      "max": 500000
    },
    "available": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "categories": {
      "collection": "category",
      "via": "products",
      "dominant": true
    },
    "expire_date": {
      "type": "datetime",
      "required": true,
      "default": "2021-08-31T05:00:00.000Z"
    }
  }
}
